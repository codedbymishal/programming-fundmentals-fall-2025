#include<stdio.h>
int main()
{
	printf("\n\t\t\t\t\tTwo Largest Numbers in an Array!\n\n");
	int i,largest,seclargest,count=0;
	int nums[10];
	for(i=0;i<10;i++)
	{
		printf("Enter number %d: ",i+1);
		scanf("%d",&nums[i]);
	}
	largest=nums[0];
	seclargest=nums[0];
	for (i = 1; i < 10; i++)
    {
        if (nums[i] > largest)
        {
            seclargest = largest;
            largest = nums[i];
        }
        else if (nums[i] > seclargest && nums[i] != largest)
        {
            seclargest = nums[i];
        }
    }
    printf("\nLargest number: %d\n", largest);
    printf("Second largest number: %d\n", seclargest);

    return 0;
}
