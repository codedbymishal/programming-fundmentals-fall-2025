#include <stdio.h>
#include <string.h>

int main() 
{
    char str1[50], str2[50];
    printf("Enter first string: ");
    fgets(str1, 50, stdin);//can store spaces too
    str1[strcspn(str1, "\n")] = 0; // remove \n and adds \0
    printf("Enter second string: ");
    fgets(str2, 50, stdin);
    str2[strcspn(str2, "\n")] = 0; 

    if (strcmp(str1, str2) == 0)
    {
    	printf("Equal\n");  	
	}       
    else
    {
    	printf("Not Equal\n");	
	}
    return 0;
}

