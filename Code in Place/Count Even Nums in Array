#include<stdio.h>
int main()
{
	printf("\n\t\t\t\t\tCount Even Numbers in an Array!\n\n");
	int i,count=0;
	int nums[10];
	for(i=0;i<10;i++)
	{
		printf("Enter number %d: ",i+1);
		scanf("%d",&nums[i]);
		if(nums[i]%2==0)
		count++;
	}
	printf("There are %d even numbers in this array!",count);
	return 0;
	
}
